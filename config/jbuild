(executables
 ((names (discover))
  (flags (:standard -safe-string))
  (libraries (base stdio configurator))))

(rule
 ((targets (xkbcommon-ccopt.sexp
            xkbcommon-cclib.sexp
            xkbcommon-ccopt
            xkbcommon-cclib))
  (deps    (discover.exe))
  (action  (run ${<} -ocamlc ${OCAMLC}))))

(jbuild_version 1)
